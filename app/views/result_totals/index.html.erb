<h1>Listing result_totals</h1>

<table>
  <thead>
    <tr>
      <th>Keyword</th>
      <th>Last searched date</th>
      <th>Max tweet</th>
      <th>Total tweet count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @result_totals.each do |result_total| %>
      <tr>
        <td><%= result_total.keyword_id %></td>
        <td><%= result_total.last_searched_date %></td>
        <td><%= result_total.max_tweet_id %></td>
        <td><%= result_total.total_tweet_count %></td>
        <td><%= link_to 'Show', result_total %></td>
        <td><%= link_to 'Edit', edit_result_total_path(result_total) %></td>
        <td><%= link_to 'Destroy', result_total, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result total', new_result_total_path %>
